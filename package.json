{
  "name": "portfolio",
  "description": "Creative Web Developer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "particlesjs": "^2.2.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  }
}
