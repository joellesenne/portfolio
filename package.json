{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Creative Web Developer",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "serve": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "particlesjs": "^2.2.3"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.11"
  }
}
